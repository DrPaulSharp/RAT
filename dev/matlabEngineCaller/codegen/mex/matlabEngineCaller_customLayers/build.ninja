# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maci64
matlabdir = /Applications/MATLAB_R2021a.app
startdir = /Users/arwel/Documents/coding/RAT/dev/matlabEngineCaller

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx10.15 clang
cxx = /usr/bin/xcrun -sdk macosx10.15 clang++
linker = /usr/bin/xcrun -sdk macosx10.15 clang
linkerxx = /usr/bin/xcrun -sdk macosx10.15 clang++

cflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.14 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c99 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$root/interface" -I "." -DMODEL=matlabEngineCaller_customLayers_mex -c 

cxxflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.14 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fobjc-arc -std=c++11 -stdlib=libc++ -DMATLAB_MEX_FILE -O2 -fwrapv -DNDEBUG -std=c++11 -I "$root" -I "$startdir" -I "$matlabdir/extern/include" -I "$root/interface" -I "." -DMODEL=matlabEngineCaller_customLayers_mex -c 

ldflags =  -Wl,-twolevel_namespace -undefined error -arch x86_64 -mmacosx-version-min=10.14 -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -bundle   -L"$matlabdir/bin/maci64" -lmx -lmex -lmat -lc++  -Wl,-rpath,@loader_path -o matlabEngineCaller_customLayers_mex.mexmaci64 -lemlrt -lcovrt -lut -lmwmathutil  "$matlabdir/bin/maci64/libeng.dylib" "$matlabdir/bin/maci64/libmx.dylib" -Wl,-exported_symbols_list,matlabEngineCaller_customLayers_mex.map

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/matlabCallFun.o : cc $startdir/matlabCallFun.c
build $builddir/matlabEngineCaller_customLayers_data.o : cc $root/matlabEngineCaller_customLayers_data.c
build $builddir/rt_nonfinite.o : cc $root/rt_nonfinite.c
build $builddir/matlabEngineCaller_customLayers_initialize.o : cc $root/matlabEngineCaller_customLayers_initialize.c
build $builddir/matlabEngineCaller_customLayers_terminate.o : cc $root/matlabEngineCaller_customLayers_terminate.c
build $builddir/matlabEngineCaller_customLayers.o : cc $root/matlabEngineCaller_customLayers.c
build $builddir/_coder_matlabEngineCaller_customLayers_api.o : cc $root/interface/_coder_matlabEngineCaller_customLayers_api.c
build $builddir/_coder_matlabEngineCaller_customLayers_mex.o : cc $root/interface/_coder_matlabEngineCaller_customLayers_mex.c
build $builddir/matlabEngineCaller_customLayers_emxutil.o : cc $root/matlabEngineCaller_customLayers_emxutil.c
build $builddir/_coder_matlabEngineCaller_customLayers_info.o : cc $root/interface/_coder_matlabEngineCaller_customLayers_info.c
build $builddir/c_mexapi_version.o : cc $matlabdir/extern/version/c_mexapi_version.c

# Link
build $root/matlabEngineCaller_customLayers_mex.mexmaci64 : link $builddir/matlabCallFun.o $builddir/matlabEngineCaller_customLayers_data.o $builddir/rt_nonfinite.o $builddir/matlabEngineCaller_customLayers_initialize.o $builddir/matlabEngineCaller_customLayers_terminate.o $builddir/matlabEngineCaller_customLayers.o $builddir/_coder_matlabEngineCaller_customLayers_api.o $builddir/_coder_matlabEngineCaller_customLayers_mex.o $builddir/matlabEngineCaller_customLayers_emxutil.o $builddir/_coder_matlabEngineCaller_customLayers_info.o $builddir/c_mexapi_version.o 
