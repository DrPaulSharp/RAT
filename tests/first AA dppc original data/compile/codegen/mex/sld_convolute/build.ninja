# CompilerName=clang
# Mode=optim
ninja_required_version = 1.3

# Basic folders
root = .
builddir = $root/build/maci64
matlabdir = /Applications/MATLAB_R2021a.app
startdir = /Users/arwel/Documents/gwaith/MD_bilayers_work/ALC collaboration/Fits/first AA dppc original data/compile

# Toolchain information
cc = /usr/bin/xcrun -sdk macosx10.15 clang
cxx = /usr/bin/xcrun -sdk macosx10.15 clang++
linker = /usr/bin/xcrun -sdk macosx10.15 clang
linkerxx = /usr/bin/xcrun -sdk macosx10.15 clang++

cflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.9 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c99 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=sld_convolute_mex -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maci64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maci64/libiomp5.dylib" 

cxxflags =  -fno-common -arch x86_64 -mmacosx-version-min=10.9 -fexceptions -isysroot /Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -fobjc-arc -std=c++11 -stdlib=libc++ -DMATLAB_MEX_FILE  -O2 -fwrapv -DNDEBUG -fvisibility=hidden -std=c++11 -I "$root" -I "$startdir" -I "$root/interface" -I "$matlabdir/extern/include" -I "." -DMODEL=sld_convolute_mex -DMW_NEEDS_VERSION_H -c -fPIC -Xpreprocessor -fopenmp -I "$matlabdir/toolbox/eml/externalDependency/omp/maci64/include" -DOpenMP_omp_LIBRARY="$matlabdir/sys/os/maci64/libiomp5.dylib" 

ldflags =  -Wl,-twolevel_namespace -undefined error -arch x86_64 -mmacosx-version-min=10.9 -Wl,-syslibroot,/Library/Developer/CommandLineTools/SDKs/MacOSX.sdk -framework Cocoa -bundle  -stdlib=libc++ -L"$matlabdir/bin/maci64" -lmx -lmex -lmat -L"$matlabdir/extern/bin/maci64" -lMatlabDataArray -lMatlabEngine  -Wl,-rpath,@loader_path -o sld_convolute_mex.mexmaci64 -L"$matlabdir/bin/maci64" -lmwblas -lemlrt -lcovrt -lut -lmwmathutil -fPIC -L"$matlabdir/sys/os/maci64" -liomp5

# Build Rules
rule cc
    command = $cc $cflags $in -o $out

rule cxx
    command = $cxx $cxxflags $in -o $out

rule link
    command = $linker $in $ldflags

rule linkxx
    command = $linkerxx $in $ldflags

# Build
build $builddir/sld_convolute_data.o : cxx $root/sld_convolute_data.cpp
build $builddir/rt_nonfinite.o : cxx $root/rt_nonfinite.cpp
build $builddir/sld_convolute_initialize.o : cxx $root/sld_convolute_initialize.cpp
build $builddir/sld_convolute_terminate.o : cxx $root/sld_convolute_terminate.cpp
build $builddir/sld_convolute.o : cxx $root/sld_convolute.cpp
build $builddir/linspace.o : cxx $root/linspace.cpp
build $builddir/eml_int_forloop_overflow_check.o : cxx $root/eml_int_forloop_overflow_check.cpp
build $builddir/interp1.o : cxx $root/interp1.cpp
build $builddir/sum.o : cxx $root/sum.cpp
build $builddir/sumMatrixIncludeNaN.o : cxx $root/sumMatrixIncludeNaN.cpp
build $builddir/anyNonFinite.o : cxx $root/anyNonFinite.cpp
build $builddir/_coder_sld_convolute_api.o : cxx $root/interface/_coder_sld_convolute_api.cpp
build $builddir/_coder_sld_convolute_mex.o : cxx $root/interface/_coder_sld_convolute_mex.cpp
build $builddir/_coder_sld_convolute_info.o : cxx $root/interface/_coder_sld_convolute_info.cpp
build $builddir/cpp_mexapi_version.o : cxx $matlabdir/extern/version/cpp_mexapi_version.cpp

# Link
build $root/sld_convolute_mex.mexmaci64 : linkxx $builddir/sld_convolute_data.o $builddir/rt_nonfinite.o $builddir/sld_convolute_initialize.o $builddir/sld_convolute_terminate.o $builddir/sld_convolute.o $builddir/linspace.o $builddir/eml_int_forloop_overflow_check.o $builddir/interp1.o $builddir/sum.o $builddir/sumMatrixIncludeNaN.o $builddir/anyNonFinite.o $builddir/_coder_sld_convolute_api.o $builddir/_coder_sld_convolute_mex.o $builddir/_coder_sld_convolute_info.o $builddir/cpp_mexapi_version.o 
