function [output,sRough] = call_customLayers(params,contrast,file,callPath,bulkIn,bulkOut,lang)


switch lang
    case 'matlab'
        [output,layers] = matlabEngineCaller_customLayers(params,contrast,file,callPath,bulkIn,bulkOut);
        sRough = params(1);
    case 'octave'
        [output,layers] = matlabEngineCaller_customLayers(params,contrast,file,callPath,bulkIn,bulkOut);
    case 'python'
end


end



